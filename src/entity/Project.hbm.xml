<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="entity.Project" table="PROJECTSPC">
		<id name="pid" type="java.lang.Integer" length="8">
			<generator class="native"></generator>
		</id>
		<property name="pname" type="java.lang.String" length="20"></property>
		<property name="pdesc" type="java.lang.String" length="500"></property>
	</class>
</hibernate-mapping>
