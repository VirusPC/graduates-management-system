<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="entity.StudentInformation" table="studentInformation">
		<id name="id" type="java.lang.String" length="8">
			<generator class="assigned"></generator>
		</id>
		<property name="name" type="java.lang.String" length="10"></property>
		<property name="graduation" type="date"></property>
		<property name="dname" type="java.lang.String" length="20"></property>
		<property name="spname" type="java.lang.String" length="20"></property>
	</class>
</hibernate-mapping>